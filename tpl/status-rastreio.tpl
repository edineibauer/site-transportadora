<li class="row no-margin" style="padding-bottom: 23px; padding-top: 10px">
    <span class="col s2 center-align" style="font-size:0.8em">#data#<br>#hora#</span>
    <div class="col s1" style="position: relative">
        <div class="risco" style="position: absolute; width: 4px; background: #DDD; height: 107px; margin-left: 13px; margin-top:-10px"></div>
        <img src="#HOME#/assets/images/status-rastreio.tpl.html" height="30" width="30" style="width: 30px; height: 30px; position:absolute;" title="Green">
    </div>
    <div class="col s9">
        <strong class="row no-margin" style="font-size:1.3em">#status#</strong>
        <div class="row no-margin">#recebido#</div>
        <small class="row no-margin">#documento#</small>
    </div>
</li>